<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.text.Text?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.sdata.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Text>Přidej Item</Text>
    <TextField text="Přidej Item"></TextField>
    <Button text="Přidej" fx:id="AddButton" onAction="#AddButtonClick"/>
    <Text>Kontrola itemu</Text>
    <TextField text="Kontrola"></TextField>
    <Button text="Zkontroluj" fx:id="KontrolaButtonClick" onAction="#KontrolaButtonClick"/>
    <TextArea fx:id="Vystup" text="Vystup"/>
</VBox>
